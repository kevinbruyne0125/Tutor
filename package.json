{
  "name": "react-redux-tutorial",
  "version": "2.0.0",
  "description": "A react redux tutorial",
  "main": "",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "uglify": "^0.1.5",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.6",
    "redux-form": "^4.1.5",
    "redux-thunk": "^1.0.3"
  },
  "scripts": {
    "watch": "watchify  static/main.js -v -t babelify -o static/bundle.js",
    "build": "browserify static/main.js -t babelify | uglifyjs -mc warnings=false > static/bundle.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spapas/react-redux-tutorial"
  },
